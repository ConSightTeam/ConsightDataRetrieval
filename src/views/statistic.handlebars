<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0 text-dark">{{ display_property }} Statistic</h1>
                </div><!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="./">Home</a></li>
                        <li class="breadcrumb-item active">{{ display_property }} Statistic</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <div>
        {{#unless statistics}}
            <p>No Data</p>
        {{/unless}}
        {{#each statistics }}
            <p>Date: {{ this.inserted_on_date }}</p>
            <p>Minimium: {{ this.minimum }}</p>
            <p>Maximium: {{ this.maximum }}</p>
            <p>Average: {{ this.average }}</p>
        {{/each}}
    </div>
    <!-- /.content -->
</div>

